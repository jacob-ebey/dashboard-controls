{
  "name": "dashboard-controls",
  "version": "0.1.8",
  "description": "Controls for creating dashboards in React",
  "main": "dist/index.js",
  "typings": "types/index.d.ts",
  "repository": "https://github.com/jacob-ebey/dashboard-controls.git",
  "author": "Jacob Ebey <jacob.ebey@live.com>",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist/ && webpack --config webpack/webpack.config.babel.js",
    "build:storybook": "build-storybook -c .storybook -o docs",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint src",
    "test": "yarn lint && jest",
    "precommit": "npm run test",
    "prepublishOnly": "yarn precommit"
  },
  "jest": {
    "verbose": true,
    "rootDir": "",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir/node_modules/"
    ],
    "testRegex": "(src|webpack).*(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0"
  },
  "devDependencies": {
    "@storybook/react": "^3.2.8",
    "@storybook/addon-actions": "^3.2.12",
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^21.0.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "enzyme": "^3.0.0",
    "enzyme-adapter-utils": "^1.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "^4.6.1",
    "eslint-config-dqnt": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.0.2",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "precss": "^2.0.0",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "timemachine": "^0.3.0",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.6",
    "webpack-features": "^1.6.4"
  }
}
